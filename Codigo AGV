/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_USART2_UART_Init();
  MX_TIM2_Init();
  MX_TIM3_Init();
  /* USER CODE BEGIN 2 */
  //Sensores
  uint32_t numTicks1 = 0;
  uint32_t numTicks2 = 0;
  uint32_t numTicks3 = 0;
  uint32_t numTicks4 = 0;
  uint32_t numTicks5 = 0;
  float distancia1;
  float distancia2;
  float distancia3;
  float distancia4;
  float distancia5;
  const float Vsonido = 0.0343f; //cm/us
  char uartBuf[50];

  #define Sensor1_PORT GPIO
  #define Sensor1_PIN  GPIO_PIN_

  #define Sensor2_PORT GPIO
  #define Sensor2_PIN  GPIO_PIN_

  #define Sensor3_PORT GPIO
  #define Sensor3_PIN  GPIO_PIN_

  #define Sensor4_PORT GPIO
  #define Sensor4_PIN  GPIO_PIN_

  #define Sensor5_PORT GPIO
  #define Sensor5_PIN  GPIO_PIN_

  #define Sensor6_PORT GPIO
  #define Sensor6_PIN  GPIO_PIN_

  #define Sensor7_PORT GPIO
  #define Sensor7_PIN  GPIO_PIN_

  #define Sensor8_PORT GPIO
  #define Sensor8_PIN  GPIO_PIN_

  #define Sensor9_PORT GPIO
  #define Sensor9_PIN  GPIO_PIN_

    // Sensor ultrasonico
  #define Sensor10_PORT GPIO
  #define Sensor10_PIN  GPIO_PIN_

  #define Sensor11_PORT GPIO
  #define Sensor11_PIN  GPIO_PIN_

  #define Sensor12_PORT GPIO
  #define Sensor12_PIN  GPIO_PIN_

  #define Sensor13_PORT GPIO
  #define Sensor13_PIN  GPIO_PIN_

  #define Sensor14_PORT GPIO
  #define Sensor14_PIN  GPIO_PIN_

  #define Sensor15_PORT GPIO
  #define Sensor15_PIN  GPIO_PIN_

  #define Sensor16_PORT GPIO
  #define Sensor16_PIN  GPIO_PIN_

  #define Sensor17_PORT GPIO
  #define Sensor17_PIN  GPIO_PIN_

  #define Sensor18_PORT GPIO
  #define Sensor18_PIN  GPIO_PIN_

  #define Sensor19_PORT GPIO
  #define Sensor19_PIN  GPIO_PIN_


       //Iniciar PWM
         HAL_TIM_PWM_Start(&htim2, TIM_CHANNEL_1);
         HAL_TIM_PWM_Start(&htim3, TIM_CHANNEL_1);

         // Puente H
         HAL_GPIO_WritePin(GPIOA, GPIO_PIN_1, GPIO_PIN_RESET);
         HAL_GPIO_WritePin(GPIOA, GPIO_PIN_4, GPIO_PIN_SET);
         HAL_GPIO_WritePin(GPIOA, GPIO_PIN_5, GPIO_PIN_RESET);\
         HAL_GPIO_WritePin(GPIOA, GPIO_PIN_7, GPIO_PIN_SET);
  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {
    /* USER CODE END WHILE */
	  //Sensores seguidores
	  GPIO_PinState sensorState1 = HAL_GPIO_ReadPin (Sensor1_PORT, Sensor1_PIN);
	  GPIO_PinState sensorState2 = HAL_GPIO_ReadPin (Sensor2_PORT, Sensor2_PIN);
	  GPIO_PinState sensorState3 = HAL_GPIO_ReadPin (Sensor3_PORT, Sensor3_PIN);
	  GPIO_PinState sensorState4 = HAL_GPIO_ReadPin (Sensor4_PORT, Sensor4_PIN);
	  GPIO_PinState sensorState5 = HAL_GPIO_ReadPin (Sensor5_PORT, Sensor5_PIN);
	  GPIO_PinState sensorState6 = HAL_GPIO_ReadPin (Sensor6_PORT, Sensor6_PIN);
	  GPIO_PinState sensorState7 = HAL_GPIO_ReadPin (Sensor7_PORT, Sensor7_PIN);
	  GPIO_PinState sensorState8 = HAL_GPIO_ReadPin (Sensor8_PORT, Sensor8_PIN);
	  GPIO_PinState sensorState9 = HAL_GPIO_ReadPin (Sensor9_PORT, Sensor9_PIN);

	  // ultrasonico
	  GPIO_PinState sensorStateTrig1 = HAL_GPIO_ReadPin (Sensor10_PORT, Sensor10_PIN);
	  GPIO_PinState sensorStateEcho1 = HAL_GPIO_ReadPin (Sensor11_PORT, Sensor11_PIN);
	  GPIO_PinState sensorStateTrig2 = HAL_GPIO_ReadPin (Sensor12_PORT, Sensor12_PIN);
	  GPIO_PinState sensorStateEcho2 = HAL_GPIO_ReadPin (Sensor13_PORT, Sensor13_PIN);
	  GPIO_PinState sensorStateTrig3 = HAL_GPIO_ReadPin (Sensor14_PORT, Sensor14_PIN);
	  GPIO_PinState sensorStateEcho3 = HAL_GPIO_ReadPin (Sensor15_PORT, Sensor15_PIN);
	  GPIO_PinState sensorStateTrig4 = HAL_GPIO_ReadPin (Sensor16_PORT, Sensor16_PIN);
	  GPIO_PinState sensorStateEcho4 = HAL_GPIO_ReadPin (Sensor17_PORT, Sensor17_PIN);
	  GPIO_PinState sensorStateTrig5 = HAL_GPIO_ReadPin (Sensor18_PORT, Sensor18_PIN);
	  GPIO_PinState sensorStateEcho5 = HAL_GPIO_ReadPin (Sensor19_PORT, Sensor19_PIN);

	  // Distancia 1
	  //Trig en bajo
	  HAL_GPIO_Write(sensorStateTrig1 == GPIO_PIN_RESET); // PA0 Trig Bajo
	  delay_us(3);

	  //Pulso de 10 us
	  HAL_GPIO_Write(sensorStateTrig1 == GPIO_PIN_SET);
	  delay_us(10);
	  HAL_GPIO_Write(sensorStateTrig1 == GPIO_PIN_RESET);

	  while (HAL_GPIO_Read(sensorStateEcho1) == GPIO_PIN_RESET);  // Esperar ECHO alto
	  numTicks1 = 0;

	  // Esperamos el ECHO
	  while (HAL_GPIO_Read(sensorStateEcho1) == GPIO_PIN_SET)
	  {
		numTicks1++;
		delay_us(2); // Aproximadamente 2.8 us
	  }

	  // Calcular distancias
	  distancia1 = (numTicks1 * 2.8f) *Vsonido/2;

	  //Distancia 2
	  //Trig en bajo
	  HAL_GPIO_Write(sensorStateTrig2, GPIO_PIN_RESET); // PA0 Trig Bajo
	  delay_us(3);

	  //Pulso de 10 us
	  HAL_GPIO_Write(sensorStateTrig2, GPIO_PIN_SET);
	  delay_us(10);
	  HAL_GPIO_Write(sensorStateTrig2, GPIO_PIN_RESET);

	  while (HAL_GPIO_Read(sensorStateEcho2) == GPIO_PIN_RESET);  // Esperar ECHO alto
	  numTicks1 = 0;

	  // Esperamos el ECHO
	  while (HAL_GPIO_Read(sensorStateEcho2) == GPIO_PIN_SET)
	  {
		numTicks2++;
		delay_us(2); // Aproximadamente 2.8 us
	  }

	  // Calcular distancias
	  distancia2 = (numTicks2 * 2.8f) *Vsonido/2;

	  //Distacia 3
	  //Trig en bajo
	  HAL_GPIO_Write(sensorStateTrig3, GPIO_PIN_RESET); // PA0 Trig Bajo
	  delay_us(3);

	  //Pulso de 10 us
	  HAL_GPIO_Write(sensorStateTrig3, GPIO_PIN_SET);
	  delay_us(10);
	  HAL_GPIO_Write(sensorStateTrig3, GPIO_PIN_RESET);

	  while (HAL_GPIO_Read(sensorStateEcho3) == GPIO_PIN_RESET);  // Esperar ECHO alto
	  numTicks3 = 0;

	  // Esperamos el ECHO
	  while (HAL_GPIO_Read(sensorStateEcho3) == GPIO_PIN_SET)
	  {
		numTicks3++;
		delay_us(2); // Aproximadamente 2.8 us
	  }

	  // Calcular distancias
	  distancia3 = (numTicks3 * 2.8f) *Vsonido/2;

	  // Distancia 4
	  //Trig en bajo
	  HAL_GPIO_Write(sensorStateTrig4, GPIO_PIN_RESET); // PA0 Trig Bajo
	  delay_us(3);

	  //Pulso de 10 us
	  HAL_GPIO_Write(sensorStateTrig4, GPIO_PIN_SET);
	  delay_us(10);
	  HAL_GPIO_Write(sensorStateTrig4, GPIO_PIN_RESET);

	  while (HAL_GPIO_Read(sensorStateEcho4) == GPIO_PIN_RESET);  // Esperar ECHO alto
	  numTicks4 = 0;

	  // Esperamos el ECHO
	  while (HAL_GPIO_Read(sensorStateEcho4) == GPIO_PIN_SET)
	  {
		numTicks4++;
		delay_us(2); // Aproximadamente 2.8 us
	  }

	  // Calcular distancias
	  distancia4 = (numTicks4 * 2.8f) *Vsonido/2;

	  //Distancia 5
	  //Trig en bajo
	  HAL_GPIO_Write(sensorStateTrig5, GPIO_PIN_RESET); // PA0 Trig Bajo
	  delay_us(3);

	  //Pulso de 10 us
	  HAL_GPIO_Write(sensorStateTrig5, GPIO_PIN_SET);
	  delay_us(10);
	  HAL_GPIO_Write(sensorStateTrig5, GPIO_PIN_RESET);

	  while (HAL_GPIO_Read(sensorStateEcho5) == GPIO_PIN_RESET);  // Esperar ECHO alto
	  numTicks5 = 0;

	  // Esperamos el ECHO
	  while (HAL_GPIO_Read(sensorStateEcho5) == GPIO_PIN_SET)
	  {
		numTicks5++;
		delay_us(2); // Aproximadamente 2.8 us
	  }

	  // Calcular distancias
	  distancia5 = (numTicks5 * 2.8f) *Vsonido/2;

	  if (distancia1 > 10.0f)
	  {
		  if ()
		  {

		  }

		  else if ()
		  {

		  }

		  else if ()
		  {

		  }

		  else if ()
		  {

		  }

		  else
		  {
			  spintf(uartBuf, "Programa bien Bitch");
			  HAL_UART_Transmit(&huart2, (uint8_t*)uartBuf, strlen(uartBuf), 100);
		  }
	  }

	  else
	  {
		  spintf(uartBuf, "Programa bien Bitch");
		  HAL_UART_Transmit(&huart2, (uint8_t*)uartBuf, strlen(uartBuf), 100);
	  }
    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}

/**
  * @brief System Clock Configuration
  * @retval None
  */
void SystemClock_Config(void)
{
  RCC_OscInitTypeDef RCC_OscInitStruct = {0};
  RCC_ClkInitTypeDef RCC_ClkInitStruct = {0};

  /** Initializes the RCC Oscillators according to the specified parameters
  * in the RCC_OscInitTypeDef structure.
  */
  RCC_OscInitStruct.OscillatorType = RCC_OSCILLATORTYPE_HSE;
  RCC_OscInitStruct.HSEState = RCC_HSE_BYPASS;
  RCC_OscInitStruct.HSEPredivValue = RCC_HSE_PREDIV_DIV1;
  RCC_OscInitStruct.HSIState = RCC_HSI_ON;
  RCC_OscInitStruct.PLL.PLLState = RCC_PLL_ON;
  RCC_OscInitStruct.PLL.PLLSource = RCC_PLLSOURCE_HSE;
  RCC_OscInitStruct.PLL.PLLMUL = RCC_PLL_MUL9;
  if (HAL_RCC_OscConfig(&RCC_OscInitStruct) != HAL_OK)
  {
    Error_Handler();
  }

  /** Initializes the CPU, AHB and APB buses clocks
  */
  RCC_ClkInitStruct.ClockType = RCC_CLOCKTYPE_HCLK|RCC_CLOCKTYPE_SYSCLK
                              |RCC_CLOCKTYPE_PCLK1|RCC_CLOCKTYPE_PCLK2;
  RCC_ClkInitStruct.SYSCLKSource = RCC_SYSCLKSOURCE_PLLCLK;
  RCC_ClkInitStruct.AHBCLKDivider = RCC_SYSCLK_DIV1;
  RCC_ClkInitStruct.APB1CLKDivider = RCC_HCLK_DIV2;
  RCC_ClkInitStruct.APB2CLKDivider = RCC_HCLK_DIV1;

  if (HAL_RCC_ClockConfig(&RCC_ClkInitStruct, FLASH_LATENCY_2) != HAL_OK)
  {
    Error_Handler();
  }
}

/**
  * @brief TIM2 Initialization Function
  * @param None
  * @retval None
  */
static void MX_TIM2_Init(void)
{

  /* USER CODE BEGIN TIM2_Init 0 */

  /* USER CODE END TIM2_Init 0 */

  TIM_MasterConfigTypeDef sMasterConfig = {0};
  TIM_OC_InitTypeDef sConfigOC = {0};

  /* USER CODE BEGIN TIM2_Init 1 */

  /* USER CODE END TIM2_Init 1 */
  htim2.Instance = TIM2;
  htim2.Init.Prescaler = 0;
  htim2.Init.CounterMode = TIM_COUNTERMODE_UP;
  htim2.Init.Period = 65535;
  htim2.Init.ClockDivision = TIM_CLOCKDIVISION_DIV1;
  htim2.Init.AutoReloadPreload = TIM_AUTORELOAD_PRELOAD_DISABLE;
  if (HAL_TIM_PWM_Init(&htim2) != HAL_OK)
  {
    Error_Handler();
  }
  sMasterConfig.MasterOutputTrigger = TIM_TRGO_RESET;
  sMasterConfig.MasterSlaveMode = TIM_MASTERSLAVEMODE_DISABLE;
  if (HAL_TIMEx_MasterConfigSynchronization(&htim2, &sMasterConfig) != HAL_OK)
  {
    Error_Handler();
  }
  sConfigOC.OCMode = TIM_OCMODE_PWM1;
  sConfigOC.Pulse = 0;
  sConfigOC.OCPolarity = TIM_OCPOLARITY_HIGH;
  sConfigOC.OCFastMode = TIM_OCFAST_DISABLE;
  if (HAL_TIM_PWM_ConfigChannel(&htim2, &sConfigOC, TIM_CHANNEL_1) != HAL_OK)
  {
    Error_Handler();
  }
  /* USER CODE BEGIN TIM2_Init 2 */

  /* USER CODE END TIM2_Init 2 */
  HAL_TIM_MspPostInit(&htim2);

}

